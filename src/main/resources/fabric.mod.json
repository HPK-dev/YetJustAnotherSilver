{
  "schemaVersion": 1,
  "id": "yjas",
  "version": "${version}",
  "name": "Yet just another silver",
  "description": "This is a simple mod to bring the silver to fabric 1.20+",
  "authors": [
    "Mike Tang",
    "iceice666"
  ],
  "contact": {
    "sources": "https://github.com/HPK-dev/YetJustAnotherSilver/"
  },
  "license": "MIT",
  "icon": "assets/yjas/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "team.hpk.yjas.ModMain",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "team.hpk.yjas.datagen.DataGenerator",
        "adapter": "kotlin"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}